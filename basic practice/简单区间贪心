#include<iostream>
#include <iomanip>
#include<cstdio>
#include<algorithm>
using namespace std;
const int maxn = 100;
struct interval
{
	int x, y;
}I[maxn];

bool cmp(interval a, interval b) {
	if (a.x != b.x) return a.x > b.x;  //左端点从大到小排
	else
		return a.y < b.y; //左端点相同你的按右端点从小到大排  优先选择占用区间更小的
}
int main() {
	int n;
	while (cin >> n, n != 0) {
		for (int i = 0; i < n; i++) {
			cin >> I[i].x >> I[i].y;
		}
		sort(I, I + n, cmp);
		int ans = 1 , lastX = I[0].x;  //lasxX记录上个选中区间的左端点
		for (int i = 1; i < n; i++) {
			if (I[i].x < lastX) {
				lastX = I[i].x;
				ans++;
			}
		}
	}

	system("pause");
	return 0;
}
