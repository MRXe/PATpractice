#include<iostream>
using namespace std;
const int maxn = 101;
bool p[maxn] = {0};
int prime[maxn], pNum = 0; //存放素数，素数个数
//筛去素数的所有倍数;
void findPrime() {
	for (int i = 2; i < maxn; i++) {
		if (p[i] == false) {
			prime[pNum++] = i;
			for (int j = i + i; j < maxn; j += i) {
				//筛去所有i的倍数
				p[j] = true;
			}
		}
	}
}

int main() {
	findPrime();
	for (int i = 0; i < pNum; i++) {
		cout << prime[i] << " " << endl;
	}
	system("pause");
	return 0;
}
